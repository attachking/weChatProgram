.container{
    padding: 30rpx 0;
    background: #f4f4f4;
}
.list .item{
    padding: 15rpx 20rpx;
    display: flex;
    align-items: center;
    background: #fff;
    font-size: 28rpx;
}
.list .item:not(:last-child){
    border-bottom: 1rpx dashed #d9d9d9;
}
.list .item .icon{
    width: 70rpx;
    margin-right: 20rpx;
}
.list .item .icon image{
    width: 100%;
}
.card{
    margin-top: 20rpx;
    background: #fff;
    font-size: 28rpx;
    padding: 20rpx;
}
.card .title{
    display: flex;
    align-items: center;
    padding: 15rpx 0;
    border-bottom: 1rpx solid #d9d9d9;
}
.card .title .ver{
    background: #409eff;
    width: 10rpx;
    height: 50rpx;
    margin-right: 15rpx;
}
.card .con{
    padding: 30rpx 0;
}
.imgBox{
    display: flex;
    align-items: center;
    justify-content: space-between;
}
.imgBox .imgItem{
    flex: 1;
    padding: 0 20rpx;
}
.imgBox image{
    width: 100%;
}